#include<stdio.h>
#include<stdlib.h>

int main()
{
	int a[5][5];
	int i,j;
	int (*p)[5][5];


	printf("enter the members of the matrix:\n");
	for(i=0;i<3;i++){
		for(j=0;j<3;j++)
		scanf("%d", &a[i][j]);
	}
	for(i=0;i<3;i++){
		printf("\n");
		for(j=0;j<3;j++)
		printf("%d\t",a[i][j]);
	}
	printf("%p\n", a);
	printf("%p\n", &a);
	printf("%p\n", &a[0][0]);
	printf("%p\n", &a[0][2]);
	printf("%p\n", &a[1][0]);
	printf("%p\n", &a[1][1]);
	printf("%p\n", &a[1][2]);
	printf("%p\n", &a[2][0]);
	printf("%p\n", &a[2][1]);
	printf("%p\n", &a[2][2]);

	a[i][j]=transpose(&a);
/*	printf("\n");
	for(i=0;i<3;i++){
		printf("\n");
		for(j=0;j<3;j++)
		printf("%d\t",a[j][i]);
	}*/

	return 0;
}
int transpose(int *p)
{
	int i,j;
	printf("&p=%p\n", &p);
	printf("p=%p\n", p);
	printf("p+1%p\n", ((p+0)+1));
/*	printf("((p+1)+0):%p\n", ((p+1)[0]));
	printf("(p+0)[0]:%d\n",*(p+0)[0]);
	printf("(p+1)[0]:%d\n",*(p+1)[0]);
	printf("(p+2)[0]:%d\n",*(p+2)[0]);
	printf("(p+0)[1]:%d\n",*(p+0)[1]);
	printf("(p+1)[1]:%d\n",*(p+1)[1]);
	printf("(p+2)[1]:%d\n",*(p+2)[1]);*/
	for(i=0;i<3;i++){
		printf("\n");
		for(j=0;j<3;j++)
			printf("%d\t",(*(p+(j*5)+i)));
	}
	return p;
}

